# Gemini API Keys
# Get your Gemini API key from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Firebase API Keys
# These are generated automatically when you configure Firebase for each platform
# You can find them in your Firebase Console under Project Settings
FIREBASE_WEB_API_KEY=your_firebase_web_api_key_here
FIREBASE_ANDROID_API_KEY=your_firebase_android_api_key_here
FIREBASE_IOS_API_KEY=your_firebase_ios_api_key_here
FIREBASE_MACOS_API_KEY=your_firebase_macos_api_key_here
FIREBASE_WINDOWS_API_KEY=your_firebase_windows_api_key_here
